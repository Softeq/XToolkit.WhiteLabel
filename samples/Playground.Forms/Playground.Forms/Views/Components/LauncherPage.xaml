<?xml version="1.0" encoding="utf-8"?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:Playground.Forms.ViewModels.Components"
    x:Class="Playground.Forms.Views.Components.LauncherPage"
    x:DataType="vm:LauncherPageViewModel">

    <StackLayout>

        <Button Text="Open Softeq Site" Command="{Binding OpenUrlCommand}" />

        <Button Text="Open App Settings" Command="{Binding OpenAppSettingsCommand}" />

        <Button Text="Open Video" Command="{Binding OpenVideoCommand}" />

        <Button Text="Open Email" Command="{Binding OpenEmailCommand}" />

        <Button Text="Open Phone Number" Command="{Binding OpenPhoneNumberCommand}" />

    </StackLayout>

</ContentPage>