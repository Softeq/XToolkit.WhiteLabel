          ]
        }
      ],
      "dest": "obj/docfxapi",
      "filter": "filterConfig.yml",
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/docfxapi",
        "dest": "api"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "articles/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_appTitle": "XToolkit.WhiteLabel",
      "_appFooter": "<span>Copyright © <a href=\"https://www.softeq.com/\">Softeq Development Corp.</a><span class=\"footer-docfx\">Generated by <strong>DocFX</strong></span></span>",
      "_gitContribute": {
        "branch": "master"
      }
    },
    "fileMetadata": {
      "priority": {
        "obj/docfxapi/**": 1.0,
        "**.md": 2.5,
        "articles/**.md": 3
      },
      "keywords": {
        "obj/docfxapi/**": [ "API", "Reference" ],
        "spec/**.md": [ "Spec", "Conceptual" ],
        "**/*markdown.md": [ "DFM", "Spec" ],
        "articles/**.md": [ "Article" ]
      }
    },
    "dest": "_site",
    "template": [
      "statictoc",
      "templates/material-classic"
    ]
  }
}