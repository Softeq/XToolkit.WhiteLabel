{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/Softeq.XToolkit.*.csproj"
          ],
          "src": "..",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "**/*.Tests.csproj",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/docfxapi"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/docfxapi",
        "dest": "api"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "articles/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_appTitle": "XToolkit.WhiteLabel",
      "_appFooter": "<span>Copyright © Softeq Development Corporation<br>Generated by <strong>DocFX</strong></span>",
      "_gitContribute": {
        "branch": "master"
      }
    },
    "dest": "_site",
    "template": [
      "statictoc",
      "templates/material"
    ]
  }
}